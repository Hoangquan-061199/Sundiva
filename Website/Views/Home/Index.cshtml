@model IndexViewModel
@using ADCOnline.Utils
@using ADCOnline.Simple.Item
@using ADCOnline.Simple.Json
@using System.Web
@{
    if (Model.SystemConfigItem != null)
    {
        ViewBag.Title = Model.SystemConfigItem.SEOTitle;
        ViewBag.Name = Model.SystemConfigItem.Name;
        ViewBag.Description = Model.SystemConfigItem.SEODescription;
        ViewBag.Keywords = Model.SystemConfigItem.SEOKeyword;
        ViewBag.UrlOgImage = Model.SystemConfigItem.ogimage;
        ViewBag.OgImageAlt = !string.IsNullOrEmpty(Model.SystemConfigItem.ogimagealt) ? Model.SystemConfigItem.ogimagealt :
        Model.SystemConfigItem.SEOTitle;
    }
    ViewBag.Index = true;
    ViewBag.IsSchema = true;
    ViewBag.Vote = "";
    Layout = "_Layout";
    ViewBag.IndexGoogle = ViewBag.NoIndex == true ? "noodp,noindex,nofollow" : Model.SystemConfigItem.IndexGoogle;
    ViewBag.Banner = true;
}

<div style="display:none">
    <vc:other-component code="TextIndex"></vc:other-component>
</div>
@if (Model.ListPositionItem.Any())
{
    @foreach (var position in Model.ListPositionItem.OrderBy(x => x.OrderDisplay))
    {
        switch (position.Code)
        {
            case StaticEnum.SlideHome:
                {
                    @if (position.AdvertisingItems.Any())
                    {
                        <div class="banner index">
                            <div class="slide-index owl-carousel owl-theme">
                                @foreach (AdvertisingItem item in position.AdvertisingItems)
                                {
                                    if (!string.IsNullOrEmpty(item.LinkUrl))
                                    {
                                        <a href="@item.LinkUrl" title="@item.Name" target="_blank" rel="nofollow" class="item">
                                            @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                        </a>
                                    }
                                    else
                                    {
                                        <div class="item">
                                            @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                        </div>
                                    }
                                }
                            </div>
                            <vc:search-component></vc:search-component>
                        </div>
                    }
                    break;
                }
            case StaticEnum.TourIndex:
                {
                    if (position.AdvertisingItems.Any())
                    {
                        <div class="type-tour">
                            <div class="container">
                                @foreach (AdvertisingItem item in position.AdvertisingItems)
                                {
                                    if (!string.IsNullOrEmpty(item.LinkUrl))
                                    {
                                        <a href="@item.LinkUrl" title="@item.Name" class="item">
                                            <div class="img">
                                                @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                            </div>
                                            <h3 class="big-title title-item">@item.Name</h3>
                                        </a>
                                    }
                                    else
                                    {
                                        <div class="item">
                                            <div class="img">
                                                @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                            </div>
                                            <h3 class="big-title title-item">@item.Name</h3>
                                        </div>
                                    }
                                }
                            </div>
                        </div>
                    }
                    break;
                }
            case StaticEnum.LastMinuteTourDeals:
                {
                    if (position.ProductItems.Any(x => x.TimeStart.Day > DateTime.Now.Day && x.TimeStart.Day <= (DateTime.Now.Day + 2)))
                    {
                        <div class="endow-tour-index">
                            <div class="container">
                                <h2 class="big-title center uppercase border bf-center">@position.Name</h2>
                                <div class="slide-endow-tour owl-carousel owl-theme">
                                    @foreach (ProductItem item in position.ProductItems.Where(x => x.TimeStart.Day > DateTime.Now.Day && x.TimeStart.Day <= (DateTime.Now.Day + 2)).OrderBy(x => x.TimeStart.Day))
                                    {
                                        var remainingTime = item.TimeStart.Day - DateTime.Now.Day;

                                        <div class="item item-tour">
                                            <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name" class="img-tour">
                                                <div class="img">
                                                    @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                                </div>
                                                <div class="info">
                                                    <div class="time">
                                                        @if (remainingTime > 0)
                                                        {
                                                            <p>@ResourceData.Resource("Con", ViewBag.Lang) @remainingTime @ResourceData.Resource("Ngay", ViewBag.Lang) - @ResourceData.Resource("Tu", ViewBag.Lang) @item.Address</p>
                                                        }
                                                        else
                                                        {
                                                            <p>@ResourceData.Resource("Tu", ViewBag.Lang) @item.Address</p>
                                                        }
                                                    </div>
                                                    <div class="price">
                                                        <p>@Utility.GetFormatPriceType(item.Price, 1, ResourceData.Resource("LienHe", ViewBag.Lang), true)</p>
                                                    </div>
                                                </div>
                                            </a>
                                            <div class="text">
                                                <h3 class="big-title title-item">
                                                    <a hrefhref="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name">
                                                        @item.Name
                                                    </a>
                                                </h3>
                                                <div class="info">
                                                    <div class="item-info">
                                                        <svg xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 512 512">
                                                            <path d="M464 256A208 208 0 1 1 48 256a208 208 0 1 1 416 0zM0 256a256 256 0 1 0 512 0A256 256 0 1 0 0 256zM232 120V256c0 8 4 15.5 10.7 20l96 64c11 7.4 25.9 4.4 33.3-6.7s4.4-25.9-6.7-33.3L280 243.2V120c0-13.3-10.7-24-24-24s-24 10.7-24 24z" />
                                                        </svg>
                                                        <p>@item.TimesValue</p>
                                                    </div>
                                                    <div class="item-info">
                                                        <svg xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 448 512">
                                                            <path d="M128 0c13.3 0 24 10.7 24 24V64H296V24c0-13.3 10.7-24 24-24s24 10.7 24 24V64h40c35.3 0 64 28.7 64 64v16 48V448c0 35.3-28.7 64-64 64H64c-35.3 0-64-28.7-64-64V192 144 128C0 92.7 28.7 64 64 64h40V24c0-13.3 10.7-24 24-24zM400 192H48V448c0 8.8 7.2 16 16 16H384c8.8 0 16-7.2 16-16V192zM329 297L217 409c-9.4 9.4-24.6 9.4-33.9 0l-64-64c-9.4-9.4-9.4-24.6 0-33.9s24.6-9.4 33.9 0l47 47 95-95c9.4-9.4 24.6-9.4 33.9 0s9.4 24.6 0 33.9z" />
                                                        </svg>
                                                        <p>@item.TimeStart.ToString("dd/MM/yyyy")</p>
                                                    </div>
                                                    <div class="item-info">
                                                        <svg xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 448 512">
                                                            <path d="M304 128a80 80 0 1 0 -160 0 80 80 0 1 0 160 0zM96 128a128 128 0 1 1 256 0A128 128 0 1 1 96 128zM49.3 464H398.7c-8.9-63.3-63.3-112-129-112H178.3c-65.7 0-120.1 48.7-129 112zM0 482.3C0 383.8 79.8 304 178.3 304h91.4C368.2 304 448 383.8 448 482.3c0 16.4-13.3 29.7-29.7 29.7H29.7C13.3 512 0 498.7 0 482.3z" />
                                                        </svg>
                                                        <p>
                                                            @if (!Utility.IsMobile(Context.Request.Headers["User-Agent"]) && !Utility.IsTablet(Context.Request.Headers["User-Agent"]))
                                                            {
                                                                <span>@ResourceData.Resource("SoChoCon",ViewBag.Lang): </span>
                                                            }
                                                            @item.Quantity
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    }
                    break;
                }
