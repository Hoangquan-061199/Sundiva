                                            </div>
                                        </a>
                                        @if (position.AdvertisingItems.Any())
                                        {
                                            <div class="list-info">
                                                @foreach (var itemchild in position.AdvertisingItems)
                                                {
                                                    if (!string.IsNullOrEmpty(itemchild.LinkUrl))
                                                    {
                                                        <a href="@itemchild.LinkUrl" title="@itemchild.Name" class="item">
                                                            <div class="icon">
                                                                @Html.Raw(Utility.GetImage(WebConfig.PathServer, itemchild.UrlPicture, itemchild.Name))
                                                            </div>
                                                            <div class="big-title title-item">
                                                                <p>@itemchild.Name</p>
                                                            </div>
                                                        </a>
                                                    }
                                                    else
                                                    {
                                                        <div class="item">
                                                            <div class="icon">
                                                                @Html.Raw(Utility.GetImage(WebConfig.PathServer, itemchild.UrlPicture, itemchild.Name))
                                                            </div>
                                                            <div class="big-title title-item">
                                                                <p>@itemchild.Name</p>
                                                            </div>
                                                        </div>
                                                    }
                                                }
                                            </div>
                                        }
                                    </div>
                                    <div class="right">
                                        @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                    </div>
                                </div>
                            </div>
                        }
                    }
                    break;
                }
            case StaticEnum.NewsIndex:
                {
                    if (position.WebsiteContentItems.Any())
                    {
                        <div class="news-index">
                            <div class="container">
                                <h2 class="big-title uppercase center border bf-center">@position.Name</h2>
                                <div class="list-news">
                                    @foreach (var item in position.WebsiteContentItems.Take(1))
                                    {
                                        <div class="left">
                                            <div class="item item-news index">
                                                <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name" class="img">
                                                    @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                                </a>
                                                <div class="text">
                                                    <div class="time">
                                                        <p>@(item.CreatedDate.HasValue ? item.CreatedDate.Value.ToString("dd/MM/yyyy") : string.Empty)</p>
                                                    </div>
                                                    <h3 class="big-title title-item">
                                                        <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name">
                                                            @item.Name
                                                        </a>
                                                    </h3>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    <div class="right">
                                        @foreach (var item in position.WebsiteContentItems.Skip(1).Take(3))
                                        {
                                            <div class="item item-news flex">
                                                <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name" class="img">
                                                    @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                                </a>
                                                <div class="text">
                                                    <div class="time">
                                                        <p>@(item.CreatedDate.HasValue ? item.CreatedDate.Value.ToString("dd/MM/yyyy") : string.Empty)</p>
                                                    </div>
                                                    <h3 class="big-title title-item">
                                                        <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name">
                                                            @item.Name
                                                        </a>
                                                    </h3>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                    break;
                }
            case StaticEnum.PartnerIndex:
                {
                    if (position.AdvertisingItems.Any())
                    {
                        <div class="partner-index">
                            <div class="container">
                                <h2 class="big-title uppercase center border bf-center">@position.Name</h2>
                                <div class="partner-scroll">
                                    <div class="list-partner">
                                        @foreach (var item in position.AdvertisingItems)
                                        {
                                            if (!string.IsNullOrEmpty(item.LinkUrl))
                                            {
                                                <a href="@item.LinkUrl" title="@item.Name" class="item">
                                                    @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                                </a>
                                            }
                                            else
                                            {
                                                <div class="item">
                                                    @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                                </div>
                                            }
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                    break;
                }
        }
    }
}