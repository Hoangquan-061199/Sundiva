                                </div>
                            </div>
                        </div>
                    }
                    break;
                }
            case StaticEnum.ServiceIndex:
                {
                    if (position.AdvertisingItems.Any())
                    {
                        <div class="service-index">
                            <div class="container">
                                <h2 class="big-title uppercase center border bf-center">@position.Name</h2>
                                <div class="list-service">
                                    @foreach (AdvertisingItem item in position.AdvertisingItems)
                                    {
                                        if (!string.IsNullOrEmpty(item.LinkUrl))
                                        {
                                            <a href="@item.LinkUrl" title="@item.Name" class="item">
                                                <div class="img">
                                                    @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                                </div>
                                                <div class="text">
                                                    <div class="icon">
                                                        @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPictureMobile, item.Name))
                                                    </div>
                                                    <h3 class="big-title uppercase white title-item-service">@item.Name</h3>
                                                </div>
                                            </a>
                                        }
                                        else
                                        {
                                            <div class="item">
                                                <div class="img">
                                                    @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                                </div>
