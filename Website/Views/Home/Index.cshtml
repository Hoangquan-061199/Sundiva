                                    }
                                </div>
                            </div>
                        </div>
                    }
                    break;
                }
            case StaticEnum.AboutIndex:
                {
                    if (position.WebsiteModulesItems.Any())
                    {
                        foreach (WebsiteModulesJson item in position.WebsiteModulesItems)
                        {
                            <div class="about-index">
                                <div class="container">
                                    <div class="left">
                                        <h2 class="big-title uppercase">@position.Name</h2>
                                        <div class="paragraph justify detail-content">
                                            @Html.Raw(item.Description)
                                        </div>
                                        <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name" class="more">
                                            <p>@ResourceData.Resource("TimHieuThem", ViewBag.Lang)</p>
                                            <div class="icon">
                                                <svg xmlns="http://www.w3.org/2000/svg" height="9px" viewBox="0 0 320 512">
                                                    <path d="M310.6 233.4c12.5 12.5 12.5 32.8 0 45.3l-192 192c-12.5 12.5-32.8 12.5-45.3 0s-12.5-32.8 0-45.3L242.7 256 73.4 86.6c-12.5-12.5-12.5-32.8 0-45.3s32.8-12.5 45.3 0l192 192z" />
