@model ADCOnline.Simple.Item.WebsiteModulesItem
@using ADCOnline.Simple.Item
@if (Model != null)
{
    <vc:bread-crumb-component module-id="@Model.ParentID"></vc:bread-crumb-component>
    if (!string.IsNullOrEmpty(Model.NameAscii) || !string.IsNullOrEmpty(Model.LinkUrl))
    {
        <a href="@Utility.Link(Model.NameAscii,string.Empty,Model.LinkUrl)" title="@Model.Name">@Model.Name</a>
        <div class="icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="8" height="8" fill="currentColor"
                 class="bi bi-chevron-right" viewBox="0 0 16 16">
                <path fill-rule="evenodd"
                      d="M4.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L10.293 8 4.646 2.354a.5.5 0 0 1 0-.708z" />
            </svg>
        </div>
    }
}