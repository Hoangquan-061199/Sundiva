                    </svg>
                    </a>*@
                        <a href="mailto:?subject=I wanted you to see this site&amp;body=Check out this site @url." class="item">
                            <svg xmlns="http://www.w3.org/2000/svg" height="13px"
                                 viewBox="0 0 512 512">
                                <path d="M48 64C21.5 64 0 85.5 0 112c0 15.1 7.1 29.3 19.2 38.4L236.8 313.6c11.4 8.5 27 8.5 38.4 0L492.8 150.4c12.1-9.1 19.2-23.3 19.2-38.4c0-26.5-21.5-48-48-48H48zM0 176V384c0 35.3 28.7 64 64 64H448c35.3 0 64-28.7 64-64V176L294.4 339.2c-22.8 17.1-54 17.1-76.8 0L0 176z" />
                            </svg>
                        </a>
                    </div>
                </div>
            </div>
            <div class="right">
                @if (Model.ListContentItemNew.Any())
                {
                    <div class="new-news">
                        <h2 class="big-title uppercase title-content border">@ResourceData.Resource("TinMoiNhat", ViewBag.Lang)</h2>
                        <div class="list-news">
                            @foreach (WebsiteContentItem item in Model.ListContentItemNew)
                            {
                                <div class="item item-news right-position">
                                    <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name" class="img">
                                        @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                    </a>
                                    <div class="text">
                                        <div class="time">
                                            <p>@(item.CreatedDate.HasValue ? item.CreatedDate.Value.ToString("dd/MM/yyyy") : string.Empty)</p>
                                        </div>
                                        <h3 class="big-title title-item">
                                            <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name">
                                                @item.Name
                                            </a>
                                        </h3>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                }
                @if (Model.ListProductHot.Any())
                {
                    <div class="outstanding-tour">
                        <h2 class="big-title uppercase title-content">@ResourceData.Resource("TourNotBat", ViewBag.Lang)</h2>
                        <div class="slide-tour-right owl-carousel owl-theme">
                            @foreach (var item in Model.ListProductHot)
                            {
                                <div class="item item-tour right-tour">
                                    <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name" class="img-tour">
                                        <div class="img">
                                            @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                                        </div>
                                        <div class="info">
                                            <div class="time">
                                                <p>@ResourceData.Resource("Tu", ViewBag.Lang) @item.Address</p>
                                            </div>
                                            <div class="price">
                                                <p>@Utility.GetFormatPriceType(item.Price, 1, ResourceData.Resource("LienHe", ViewBag.Lang), true)</p>
                                            </div>
                                        </div>
                                    </a>
                                    <div class="text">
                                        <h3 class="big-title title-item">
                                            <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name">
                                                @item.Name
                                            </a>
                                        </h3>
                                        <div class="info">
                                            @if (!string.IsNullOrEmpty(item.TimesValue))
                                            {
                                                <div class="item-info">
                                                    <svg xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 512 512">
                                                        <path d="M464 256A208 208 0 1 1 48 256a208 208 0 1 1 416 0zM0 256a256 256 0 1 0 512 0A256 256 0 1 0 0 256zM232 120V256c0 8 4 15.5 10.7 20l96 64c11 7.4 25.9 4.4 33.3-6.7s4.4-25.9-6.7-33.3L280 243.2V120c0-13.3-10.7-24-24-24s-24 10.7-24 24z" />
                                                    </svg>
                                                    <p>@item.TimesValue</p>
                                                </div>
                                            }
                                            @if (item.TimeStart.HasValue)
                                            {
                                                <div class="item-info">
                                                    <svg xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 448 512">
                                                        <path d="M128 0c13.3 0 24 10.7 24 24V64H296V24c0-13.3 10.7-24 24-24s24 10.7 24 24V64h40c35.3 0 64 28.7 64 64v16 48V448c0 35.3-28.7 64-64 64H64c-35.3 0-64-28.7-64-64V192 144 128C0 92.7 28.7 64 64 64h40V24c0-13.3 10.7-24 24-24zM400 192H48V448c0 8.8 7.2 16 16 16H384c8.8 0 16-7.2 16-16V192zM329 297L217 409c-9.4 9.4-24.6 9.4-33.9 0l-64-64c-9.4-9.4-9.4-24.6 0-33.9s24.6-9.4 33.9 0l47 47 95-95c9.4-9.4 24.6-9.4 33.9 0s9.4 24.6 0 33.9z" />
                                                    </svg>
                                                    <p>@item.TimeStart.Value.ToString("dd/MM/yyyy")</p>
                                                </div>
                                            }
                                            @if (item.Quantity > 0)
                                            {
                                                <div class="item-info">
                                                    <svg xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 448 512">
                                                        <path d="M304 128a80 80 0 1 0 -160 0 80 80 0 1 0 160 0zM96 128a128 128 0 1 1 256 0A128 128 0 1 1 96 128zM49.3 464H398.7c-8.9-63.3-63.3-112-129-112H178.3c-65.7 0-120.1 48.7-129 112zM0 482.3C0 383.8 79.8 304 178.3 304h91.4C368.2 304 448 383.8 448 482.3c0 16.4-13.3 29.7-29.7 29.7H29.7C13.3 512 0 498.7 0 482.3z" />
                                                    </svg>
                                                    <p>
                                                        @item.Quantity
                                                    </p>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
    @if (Model.ListContentItem.Any())
    {
        <div class="related-tour related-news">
            <div class="container">
                <h2 class="big-title uppercase border">@ResourceData.Resource("TinTucLienQuan", ViewBag.Lang)</h2>
                <div class="slide-endow-tour owl-carousel owl-theme">
                    @foreach (WebsiteContentItem item in Model.ListContentItem)
                    {
                        <div class="item item-news">
                            <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name" class="img">
                                @Html.Raw(Utility.GetImage(WebConfig.PathServer, item.UrlPicture, item.Name))
                            </a>
                            <div class="text">
                                <div class="time">
                                    <p>@(item.CreatedDate.HasValue ? item.CreatedDate.Value.ToString("dd/MM/yyyy") : string.Empty)</p>
                                </div>
                                <h3 class="big-title title-item">
                                    <a href="@Utility.Link(item.NameAscii, string.Empty, item.LinkUrl)" title="@item.Name">
                                        @item.Name
                                    </a>
                                </h3>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    }

    <vc:other-component code="PluginFacebook"></vc:other-component>
}
